#include <iostream>

using namespace std;

int main()
{
   int n,k=1,l=0,j=0;
    cin>>n;
    int a[n],b[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            b[j]=k;
        }
        else{
            b[j]=l;
        }
        j++;
    }   
for(int j=0;j<n;j++)
{
    cout<<b[j];
}
}
